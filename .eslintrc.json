{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-unused-vars": [
            1
        ],
        "camelcase": 0,
        "func-names": 0,
        "no-param-reassign": 0
    }
}